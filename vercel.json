{
  "version": 2,
  "builds": [
    {
      "src": "ecommerce-server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "ecommerce-server/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "front/$1"
    }
  ]
}